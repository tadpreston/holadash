<h1>Employee Listing</h1>

<table>
  <tr>
    <th>Employee ID</th>
    <th>Username</th>
    <th>Email</th>
    <th>First name</th>
    <th>Last name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.employee_id %></td>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= link_to 'Show', [:administration,user] %></td>
    <td><%= link_to 'Edit', edit_administration_user_path(user) %></td>
    <td><%= link_to 'Destroy', [:administration,user], confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Employee', new_administration_user_path %>
